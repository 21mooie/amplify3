{
	"storage": {
		"amplify3dynamodb": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"amplify3lambda": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "amplify3dynamodb",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"api": {
		"amplify3api": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "amplify3lambda",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}